<% content_for :menu do%>
<li><%=link_to "돌아가기",data_menulists_path(:page => @page)%></li>
<%end%>
<div class="container">
<h3>메뉴번역추가</h3>
        
  <%=form_for @menulist, url: data_menulists_path do |f|%>
    <%=f.text_field :page,value: @page,type: :hidden%>
    <div class="input-field">
      <%=f.label "한글이름"%>
      <%=f.text_field :kname%>
    </div>
    <div class="input-field">
      <%=f.label "영어 뜻"%>
      <%=f.text_field :ename%>
    </div>
    <div class="input-field">
      <%=f.label "독음"%>
      <%=f.text_field :ername%>
    </div>
    <div class="input-field">
      <%=f.label "일어 훈독"%>
      <%=f.text_field :jnamea%>
    </div>
    <div class="input-field">
      <%=f.label "중국어 간체"%>
      <%=f.text_field :cname%>
    </div>
    <div class="input-field">
      <%=f.label "중국어 번체"%>
      <%=f.text_field :cnameb%>
    </div>
    <div class="input-field">
      <%=f.label "아랍어"%>
      <%=f.text_field :aname%>
    </div>
    <div class="input-field">
      <%=f.label "ES"%>
      <%=f.text_field :spanish%>
    </div>
    <div class="input-field">
      <%=f.label "DE"%>
      <%=f.text_field :germany%>
    </div>
    <div class="input-field">
      <%=f.label "ITA"%>
      <%=f.text_field :italia%>
    </div>
    <div class="input-field">
      <%=f.label "PORTU"%>
      <%=f.text_field :portugal%>
    </div>
    <div class="input-field">
      <%=f.label "french"%>
      <%=f.text_field :french%>
    </div>
    <div class="input-field">
      <%=f.label "esperanto"%>
      <%=f.text_field :esperanto%>
    </div>
    <div class="input-field">
      <%=f.label "picture name"%>
      <%=f.text_field :u_picture%>
    </div>
    <%=f.submit "저장",class: :btn%>
  <%end%>
</div>
<br>
<script>
  $(".button-collapse").sideNav();
</script>